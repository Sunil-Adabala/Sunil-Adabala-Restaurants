<%- include("partials/headers"); -%>


<%# <h1>hey!</h1> %>
<div class="container">

  <header class="jumbotron" style="padding-top: 20px;">
    <div class="container">
      <h1>Restaurants</h1>
      <p><a class="btn btn-primary btn-large" href="/restaurants/new">Add new restaurants</a></p>
    </div>
  </header>
  <div class="row" style="display:flex;flex-wrap:wrap;">
    <% restaurants.forEach(function(restaurants){%>
    <div class="col-md-3 col-sm-6">
      <div class="thumbnail">
        <img src="<%=restaurants.image%>" />

        <div class="caption">
          <h4><%=restaurants.name%></h4>
        </div>
        <p>
        <a href="/restaurants/<%=restaurants._id%>" class="btn btn-primary">More info</a>
        </p>
      </div>
    </div>

    <% }); %>
  </div>



  <%# <a href="/">landing</a> %>

</div>


